@{
    ViewData["Title"] = "Home Page";
}
@model BookListViewModel

<div>
    <div class="">
        <!--Loop through each book in the repository sent to the view-->
        <br /><br />
        @foreach (var x in Model.Books)
        {
            <partial name="ProjectSummary" model="x" />
        }
    </div>
    <div page-model="@Model.PagingInfo" page-action="Index" page-classes-enabled="true" page-class="btn" page-class-normal="btn-outline-dark" page-class-selected="btn-info" page-url-category="@Model.CurrentCategory" class="btn-group pull-right m-1"></div>
</div>
